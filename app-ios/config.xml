<?xml version='1.0' encoding='utf-8'?>
<widget id="com.yoyow.app" ios-CFBundleVersion="2" version="1.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/../app-res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>YOYOW</name>
    <description>
        YOYOW Wallet APP.
    </description>
    <author email="dev@yoyow.org" href="http://wallet.yoyow.org">
        YOYOW Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="ios">
        <allow-navigation href="*" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <edit-config file="*-Info.plist" mode="merge" target="NSCameraUsageDescription">
            <string>App需要您的同意,才能访问相机</string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="Privacy - Photo Library Additions Usage Description">
            <string>
            </string>
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryUsageDescription">
            <string />
        </edit-config>
        <edit-config file="*-Info.plist" mode="merge" target="NSPhotoLibraryAddUsageDescription">
            <string />
        </edit-config>
        <config-file parent="CFBundleURLTypes" target="*-Info.plist">
            <array>
                <dict>
                    <key>CFBundleURLName</key>
                    <string />
                    <key>CFBundleURLSchemes</key>
                    <array>
                        <string>yoyow</string>
                    </array>
                </dict>
            </array>
        </config-file>
        <icon height="20" src="../app-res/icon/ios/icon-20.png" width="20" />
        <icon height="29" src="../app-res/icon/ios/icon-small.png" width="29" />
        <icon height="57" src="../app-res/icon/ios/icon-57.png" width="57" />
        <icon height="58" src="../app-res/icon/ios/icon-small@2x.png" width="58" />
        <icon height="87" src="../app-res/icon/ios/icon-small@3x.png" width="87" />
        <icon height="40" src="../app-res/icon/ios/icon-40.png" width="40" />
        <icon height="80" src="../app-res/icon/ios/icon-40@2x.png" width="80" />
        <icon height="50" src="../app-res/icon/ios/icon-50.png" width="50" />
        <icon height="100" src="../app-res/icon/ios/icon-50@2x.png" width="100" />
        <icon height="114" src="../app-res/icon/ios/icon-57@2x.png" width="114" />
        <icon height="60" src="../app-res/icon/ios/icon-60.png" width="60" />
        <icon height="120" src="../app-res/icon/ios/icon-60@2x.png" width="120" />
        <icon height="180" src="../app-res/icon/ios/icon-60@3x.png" width="180" />
        <icon height="72" src="../app-res/icon/ios/icon-72.png" width="72" />
        <icon height="144" src="../app-res/icon/ios/icon-72@2x.png" width="144" />
        <icon height="76" src="../app-res/icon/ios/icon-76.png" width="76" />
        <icon height="152" src="../app-res/icon/ios/icon-76@2x.png" width="152" />
        <icon height="1024" src="../app-res/icon/ios/icon-1024.png" width="1024" />
        <icon height="167" src="../app-res/icon/ios/icon-83d5@2x.png" width="167" />
        <preference name="WebViewBounce" value="false" />
        <preference name="DisallowOverscroll" value="true" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <feature name="CDVWKWebViewEngine">
        <param name="ios-package" value="CDVWKWebViewEngine" />
    </feature>
    <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
    <preference name="SplashScreenDelay" value="300000" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="FadeSplashScreenDuration" value="500" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="orientation" value="portrait" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="org.devgeeks.Canvas2ImagePlugin" spec="https://github.com/devgeeks/Canvas2ImagePlugin.git" />
    <plugin name="cordova-plugin-decimal-keyboard" spec="https://github.com/simanaitis/cordova-plugin-decimal-keyboard.git" />
    <plugin name="cordova-plugin-network-information" spec="^2.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.1" />
    <plugin name="cordova-plugin-ionic-webview" spec=">=2.2.0" />
    <plugin name="phonegap-plugin-barcodescanner" spec="https://github.com/bulangnisi/phonegap-plugin-barcodescanner.git" />
    <chcp>
        <auto-download enabled="false" />
        <auto-install enabled="false" />
        <native-interface version="2" />
        <config-file url="http://192.168.1.167:8002/app/ios/www/chcp.json" />
    </chcp>
    <plugin name="cordova-plugin-native-spinner" spec="https://github.com/bulangnisi/cordova-plugin-native-spinner.git" />
    <plugin name="com.lampa.startapp" spec="https://github.com/lampaa/com.lampa.startapp.git" />
    <plugin name="cordova-plugin-appavailability" spec="^0.4.2" />
    <plugin name="cordova-plugin-inappbrowser" spec="^3.0.0" />
    <plugin name="cordova-hot-code-push-plugin" spec="https://github.com/4550155/cordova-hot-code-push.git" />
    <plugin name="cordova-plugin-http" spec="https://github.com/Switch168/cordova-HTTP.git" />
    <plugin name="cordova-plugin-sslcertificatechecker" spec="https://github.com/EddyVerbruggen/SSLCertificateChecker-PhoneGap-Plugin" />
    <plugin name="org.frd49.cordova.exitapp" spec="https://github.com/FRD49/iOS-ExitApp.git" />
    <engine name="ios" spec="^4.5.4" />
</widget>
