{
  "name": "YOYOW-APP",
  "displayName": "YOYOW-APP",
  "version": "0.1.2",
  "description": "YOYOW钱包APP",
  "main": "./main.js",
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "clean-webpack-plugin": "^0.1.16",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.26.1",
    "es6-promise": "^4.1.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.9.0",
    "git-rev-sync": "^1.8.0",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.4",
    "jsx-loader": "^0.13.2",
    "node-sass": "^4.7.2",
    "postcss-loader": "^1.2.2",
    "remarkable-loader": "^0.2.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.0",
    "worker-loader": "^0.7.1"
  },
  "scripts": {
    "profile-dev": "webpack --env.dev --env.profile --profile --json > stats-dev.json",
    "profile-build": "cross-env NODE_ENV=production webpack --env.prod --env.profile --profile --json > stats-prod.json",
    "profile-nougly": "cross-env NODE_ENV=production webpack --env.prod --env.profile --env.noUgly --profile --json > stats-prod-nougly.json",
    "build": "cross-env NODE_ENV=production webpack --env.prod --progress --display-error-details --colors",
    "build-hash": "cross-env NODE_ENV=production webpack --env.prod --env.hash --env.baseUrl='' --progress --display-error-details --colors",
    "build-app": "cross-env NODE_ENV=production webpack --env.prod --env.hash --env.app --env.baseUrl='' --progress --display-error-details --colors",
    "postbuild-app": "copyfiles -f ./app-source/* ./app-android/www/ & copyfiles -f ./app-source/img/* ./app-android/www/img/ & copyfiles -f ./app-source/* ./app-ios/www/ & copyfiles -f ./app-source/img/* ./app-ios/www/img/",
    "build-hash-test": "cross-env NODE_ENV=production webpack --env.prod --env.hash --env.test --env.baseUrl='' --progress --display-error-details --colors",
    "build-app-test": "cross-env NODE_ENV=production webpack --env.prod --env.hash --env.app --env.test --env.baseUrl='' --progress --display-error-details --colors",
    "postbuild-app-test": "copyfiles -f ./app-source/* ./app-android/www/ & copyfiles -f ./app-source/img/* ./app-android/www/img/ & copyfiles -f ./app-source/* ./app-ios/www/ & copyfiles -f ./app-source/img/* ./app-ios/www/img/",
    "start": "cross-env NODE_ENV=development node server.js",
    "test:fetchApi": "cross-env NODE_ENV=test mocha -t 10000 --require babel-core/register ./test/api/fetchApi.test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yoyow-org/yoyow-app.git"
  },
  "keywords": [
    "yoyow",
    "ui",
    "app"
  ],
  "author": "YOYOW Developer TEAM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yoyow-org/yoyow-app/issues"
  },
  "homepage": "https://yoyow.org",
  "dependencies": {
    "alt": "^0.18.6",
    "alt-container": "^1.0.2",
    "alt-react": "0.0.1",
    "antd-mobile": "^2.2.2",
    "babel-polyfill": "^6.26.0",
    "bignumber.js": "^4.0.0",
    "clipboard": "^2.0.0",
    "counterpart": "^0.18.2",
    "exif-js": "^2.1.1",
    "file-saver": "^1.3.3",
    "fractional": "^1.0.0",
    "history": "^3.2.1",
    "html2canvas": "^1.0.0-alpha.12",
    "immutable": "^3.8.1",
    "indexeddbshim": "^2.2.1",
    "intl": "^1.2.5",
    "isomorphic-fetch": "^2.2.1",
    "js-sha256": "^0.2.3",
    "keccak": "^1.3.0",
    "lodash": "^4.17.5",
    "lz-string": "^1.4.4",
    "lzma": "2.1.6",
    "mocha": "^5.2.0",
    "numeral": "^2.0.4",
    "pica": "^2.0.8",
    "qr.js": "0.0.0",
    "qrcode-reader": "^0.2.2",
    "qrious": "^4.0.2",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-interpolate-component": "^0.11.0",
    "react-intl": "^2.2.0",
    "react-notification-system": "^0.2.11",
    "react-qr-reader": "git+https://github.com/JodusNodus/react-qr-reader.git",
    "react-router": "^3.0.0",
    "react-translate-component": "^0.14.0",
    "tcomb": "^3.2.16",
    "whatwg-fetch": "^2.0.2",
    "yoyowjs-lib": "git+https://github.com/yoyow-org/yoyowjs-lib.git"
  }
}
